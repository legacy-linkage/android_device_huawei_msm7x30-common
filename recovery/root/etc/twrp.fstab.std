# TWRP fstab file.

#<mnt_point>	<type>		<src>						<flags>
/cust		vfat		/dev/block/mmcblk0p1				flags=backup=y;
/system		ext4		/dev/block/mmcblk0p12
/cache		ext4		/dev/block/mmcblk0p6
/data		ext4		/dev/block/mmcblk0p13				length=-16384
/external_sd	vfat		/dev/block/mmcblk1p1				flags=display="Micro SDcard";storage;wipeingui;removable
